<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Use this flag to disable auto backup on Android 12+ -->
        <include domain="root" />
        <exclude domain="root" path="device_encrypted_files" />
    </cloud-backup>
    <device-transfer>
        <include domain="root" />
        <exclude domain="root" path="device_encrypted_files" />
    </device-transfer>
</data-extraction-rules>